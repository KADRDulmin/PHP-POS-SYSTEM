        "ext-pdo": "*",
        "ext-pdo_mysql": "*"
    },
    "require-dev": {
        "heroku/heroku-buildpack-php": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
